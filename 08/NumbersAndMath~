public class NumbersAndMath {
    public static void main(String[] args) {
        System.out.println("I will now count my victims:");
        
        System.out.println("Women " + (25 + 30 / 6));
        System.out.println("Men " + (100 - 25 * 3 % 4));
        
        System.out.println("Now I will count the children:");
        
        System.out.println(3 + 2 + 1 - 5 + 4 % 2 - 1 / 4 + 6);
        
        System.out.println("Is it true that 3 + 2 < 5 - 7?");
    }
}
